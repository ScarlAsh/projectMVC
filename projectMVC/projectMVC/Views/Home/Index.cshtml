        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="intro-slider-container slider-container-ratio mb-2 mb-lg-0">
                        <div class="intro-slider owl-carousel owl-simple owl-dark owl-nav-inside" data-toggle="owl" data-owl-options='{
                                        "nav": false,
                                        "dots": true,
                                        "responsive": {
                                            "768": {
                                                "nav": true,
                                                "dots": false
                                            }
                                        }
                                    }'>
                            @for (int i = 1; i <= 10; i++)
                            {
                                <div class="intro-slide">
                                    <figure class="slide-image">
                                        <picture>
                                                <source media="(max-width: 480px)" srcset="@Url.Content("~/assets/images/demos/demo-3/slider/" + i + ".jpg")">
                                                        <img srcset="@Url.Content("~/assets/images/demos/demo-3/slider/" + i + ".jpg")" alt="Image Desc">
                                           

                                        </picture>
                                    </figure>

                           
                                </div><!-- End .intro-slider owl-carousel owl-simple -->
                            }

                        <span class="slider-loader"></span><!-- End .slider-loader -->
                    </div><!-- End .intro-slider-container -->
                </div><!-- End .col-lg-8 -->

            </div><!-- End .container -->
        </div><!-- End .intro-section -->

